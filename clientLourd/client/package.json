{
  "name": "client",
  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "format": "prettier --config ../.prettierrc.yaml --write \"src/**/*\"",
    "build": "ng build",
    "test": "ng test",
    "coverage": "ng test --code-coverage --sourceMap=true",
    "lint": "ng lint --type-check --format stylish",
    "lint:fix": "npm run lint -- --fix",
    "start": "ng serve --open",
    "sPython": "python ../../serveur/notskribbl-server/server.py",
    "elect": "ng build --base-href ./ && electron .",
    "build:electron": "ng build --prod && electron-packager .  --asar=true --ignore=/node_modules --ignore=/e2e --ignore=/src --overwrite --out=./build",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.1.2",
