    "ng-mocks": "^10.5.2",
    "ng-multiselect-dropdown": "^0.2.10",
    "ng-select": "^1.0.2",
    "ngx-electron": "^2.2.0",
    "potrace": "^2.1.8",
    "rxjs": "~6.6.3",
    "socket.io": "^3.0.1",
